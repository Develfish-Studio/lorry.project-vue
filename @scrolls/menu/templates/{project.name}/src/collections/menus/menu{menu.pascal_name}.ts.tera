  title: '{{menu.title_name}}',
  items: [
    {%- for item in menu.items %}
    { type: 'page', label: '{{item.item.title_name}}', description: 'View {{menu.title_name}} â†’ {{item.item.title_name}} page', execute: () => router.push('{{item.to}}') },
    {%- endfor %}
  ]
})
